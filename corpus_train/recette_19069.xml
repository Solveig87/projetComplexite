<?xml version="1.0" encoding="utf-8"?>
<recette id="19069">
  <titre>Cake à la macédoine</titre>
  <type>Entrée</type>
  <niveau>Très facile</niveau>
  <cout>Bon marché</cout>
  <ingredients>
    <p>1 boîte macédoine légumes (400 g environ)</p>
    <p>4 oeufs</p>
    <p>1 paquet de gruyère râpé (125 g)</p>
    <p>1 pot crème fraîche (25 cl)</p>
    <p>3 cuillères à soupe de farine</p>
    <p>sel poivre</p>
  </ingredients>
  <preparation>
  <![CDATA[
    A préparer la veille. 
    Mélanger le tout. 
    Disposer dans un moule à cake. 
    Mettre au four 35 min. 
    Laisser refroidir et mettre au frigo
  ]]>
  </preparation>
</recette>
