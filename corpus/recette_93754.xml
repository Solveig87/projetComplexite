<recette id="93754">
  <titre>Cake ''doudou''</titre>
  <type>Dessert</type>
  <niveau>Très facile</niveau>
  <cout>Moyen</cout>
  <ingredients>
    <p>6 jaunes d'œuf</p>
    <p>	- 14 cuillères à soupe de faisselle</p>
    <p>	- 8 cuillères à soupe de maïzena </p>
    <p>	- 60g de raisins secs</p>
    <p>	- 100g de sucre</p>
  </ingredients>
